{
    "name": "@aminnairi/math",
    "version": "0.1.0",
    "description": "Reliable add, divide, multiply & subtract results.",
    "homepage": "https://github.com/aminnairi/math#readme",
    "module": "./lib/math.esm.js",
    "main": "./lib/math.common.js",
    "browser": "./lib/math.browser.js",
    "license": "MIT",
    "scripts": {
        "test": "nyc mocha",
        "build": "rollup --config rollup.config.js"
    },
    "keywords": [
        "math",
        "add",
        "divide",
        "multiply",
        "subtract"
    ],
    "files": [
        "./src/math.js",
        "./src/utils.js",
        "./lib/math.common.js",
        "./lib/math.browser.js",
        "./lib/math.esm.js"
    ],
    "private": false,
    "bugs": {
        "url": "https://github.com/aminnairi/math/issues"
    },
    "author": {
        "name": "Amin NAIRI",
        "url": "https://github.com/aminnairi"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/aminnairi/math.git"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/preset-env": "^7.9.0",
        "@babel/register": "^7.9.0",
        "@istanbuljs/nyc-config-babel": "^3.0.0",
        "@rollup/plugin-commonjs": "^11.0.2",
        "@rollup/plugin-node-resolve": "^7.1.1",
        "chai": "^4.2.0",
        "mocha": "^7.1.1",
        "nyc": "^15.0.0",
        "rollup": "^2.1.0",
        "rollup-plugin-terser": "^5.3.0"
    },
    "browserslist": [
        "> 5%",
        "not dead"
    ],
    "babel": {
        "presets": [
            "@babel/preset-env"
        ]
    },
    "nyc": {
        "require": "@babel/register",
        "all": true,
        "extension": [
            ".mjs"
        ],
        "include": [
            "src"
        ],
        "check-coverage": true,
        "excludeNodeModules": true,
        "reporter": [
            "text",
            "html"
        ]
    },
    "dependencies": {}
}
